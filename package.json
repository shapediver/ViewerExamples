{
  "name": "viewerexamples",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run tsc-check && npm run search && npm run parcel-build && npm run copy-readme && npm run add-html && npm run parcel-open",
    "copy-readme": "find ./examples -type f -name 'README.md' -exec cp --parents {} ./dist \\;",
    "tsc-check": "tsc --noEmit",
    "search": "ts-node ./scripts/searchForExampleFiles.ts",
    "add-html": "ts-node ./scripts/addHtml.ts",
    "parcel-build": "parcel build index.html 'examples/**/*.html'",
    "parcel-open": "parcel index.html --open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shapediver/viewer": "2.12.8",
    "@shapediver/viewer.features.attribute-visualization": "2.12.8",
    "@shapediver/viewer.features.interaction": "2.12.8",
    "codesandbox": "^2.2.3",
    "marked": "^12.0.2",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
