{
  "name": "viewerexamples",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "start": "npm run tsc-check && npm run before-build && parcel build index.html 'examples/**/*.html' && npm run after-build && npm run parcel-open",
    "build": "npm run tsc-check && npm run before-build && find examples -name 'example.html' -exec sh -c 'mkdir -p \"dist/$(dirname \"{}\")\" && parcel build \"{}\" --out-dir \"dist/$(dirname \"{}\")\" --public-url ./' \\; && parcel build index.html --public-url ./ && npm run after-build",
    "tsc-check": "tsc --noEmit",
    "before-build": "rm -rf ./dist && ts-node ./scripts/beforeBuild.ts",
    "after-build": "ts-node ./scripts/afterBuild.ts",
    "parcel-build-prod-main": "parcel build index.html --public-url ./",
    "parcel-build-local": "parcel build index.html",
    "parcel-open": "parcel index.html --open --port 8080"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shapediver/viewer": "2.12.8",
    "@shapediver/viewer.shared.demo-helper": "2.12.8",
    "@shapediver/viewer.features.attribute-visualization": "2.12.8",
    "@shapediver/viewer.features.interaction": "2.12.8",
    "@types/three": "0.152.0",
    "postprocessing": "6.33.1",
    "suncalc": "^1.9.0",
    "three": "0.152.2"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "codesandbox": "^2.2.3",
    "jsdom": "^24.0.0",
    "marked": "^12.0.2",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
